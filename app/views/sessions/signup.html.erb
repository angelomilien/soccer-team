<h1>Signup here<h1><br>

<%= form_for @user, url: '/signup' do |f|%>

   <%= f.label :username %>
   <%= f.text_field :user_name%><br>

   <%= f.label :email %>
   <%= f.email_field :email%><br>

   <%= f.label :password %>
   <%= f.password_field :password%><br><br>

   <%= f.submit "Submit" %>
<%end%>